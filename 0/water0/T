FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/water0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 373.15;

boundaryField
{
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
    wedgeleft
    {
        type            zeroGradient;
        value           uniform 373.15;
    }
    water0_to_water1
    {
        type            calculated;
        value           uniform 373.15;
    }
    water0_to_cra4
    {
        type            calculated;
        value           uniform 373.15;
    }

}


