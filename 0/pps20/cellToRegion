FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/pps20";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];


internalField   uniform 20;

boundaryField
{
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
    wedgeleft
    {
        type            zeroGradient;
    }
    pps20_to_pps21
    {
        type            calculated;
        value           uniform 0;
    }
    pps20_to_ppf24
    {
        type            calculated;
        value           uniform 0;
    }
    pps20_to_adh16
    {
        type            calculated;
        value           uniform 0;
    }
}


